@model Chavo.Web.Data.Entity.InvestorProduct

@{
    ViewBag.Title = "Details";
    ViewBag.Title = "Investor Product";
}

<h2>@ViewBag.Title</h2>
<h4>@ViewBag.SubTitle</h4>
<ol class="breadcrumb">
    <li><a href="@Url.Action("Index","Home")">Inicio</a></li>
    <li><a href="@Url.Action("Index")">Customers</a></li>
    <li><a href="@Url.Action("Details", new { id=Model.Investor.CustomerId})">@Model.Investor.Customer.AllName</a></li>
    <li class="active">@ViewBag.Title</li>
</ol>
<hr />
<div id="myForm">
    @{ Html.RenderPartial("_InvestorProductItems", Model); }
</div>
<div class="col-md-6">
    <div class="form-horizontal">
        <div class="form-group">
            <a href="@Url.Action("EditInvestorProduct",new { id=Model.InvestorProductId})" class="btn btn-warning"><i class="fa fa-pencil"></i>&nbsp;Edit</a>
            <a href="@Url.Action("Details", new { id=Model.Investor.CustomerId})" class="btn btn-secondary"><i class="fa fa-arrow-left"></i>&nbsp;To Back</a>
        </div>
    </div>
</div>